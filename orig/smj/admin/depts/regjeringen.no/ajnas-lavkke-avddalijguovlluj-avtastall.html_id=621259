}
function nettby_click(pageurl,pagename)
{
    wnd=window.open('http://www.nettby.no/user/edit_link.php?name=regjeringen.no&url='+pageurl+'&description='+pagename,'edit_link','scrollbars=no,width=450,height=430');
    wnd.focus();
    return false;
}
function GetThis(T, C, U, L) 
{
	var targetUrl = 'http://www.myspace.com/index.cfm?fuseaction=postto&' + 't=' + encodeURIComponent(T) + '&c=' + encodeURIComponent(C) + '&u=' + encodeURIComponent(U) + '&l=' + L;
	wnd = window.open(targetUrl);
	wnd.focus();
	return false;
}

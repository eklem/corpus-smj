        <em type="bold">Jåhttesámij lissta </em>
      </p>
      <p type="listitem">
        <em type="bold">Oalgesbelludahka </em>
      </p>
      <p type="listitem" xml:lang="sme">
        <em type="bold">Nordkalottfolket </em>
      </p>
